{
  "extends": "google",

  "rules": {
      "no-tabs": 0,
      "valid-jsdoc": 0,
      "arrow-parens": 0,
      "require-jsdoc": 0,
      "object-curly-spacing": 0,
      "indent": ["error", "tab"],
      "prefer-promise-reject-errors": 0,
      "max-len": ["error", { "code": 120 }]
  },

  "parserOptions": {
      "ecmaVersion": 2018
  },

  "env": {
      "es6": true
  }    
}